{
  "name": "pre-commit-with-lint",
  "version": "1.2.5",
  "description": "fork from pre-commit and add lint to lint changed files.",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- test.js",
    "example-fail": "echo \"This is the example hook, I exit with 1\" && exit 1",
    "example-pass": "echo \"This is the example hook, I exit with 0\" && exit 0",
    "install": "node install.js",
    "test": "mocha test.js",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- test.js",
    "uninstall": "node uninstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dyw934854565/pre-commit.git"
  },
  "keywords": [
    "git",
    "hooks",
    "npm",
    "pre-commit",
    "precommit",
    "run",
    "test",
    "development"
  ],
  "author": "Arnout Kazemier <opensource@observe.it> duanyuwen <dyw934854565@sina.com>",
  "homepage": "https://github.com/dyw934854565/pre-commit",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.3.0",
    "cross-spawn": "^5.0.1",
    "lodash.assign": "^4.2.0",
    "lodash.foreach": "^4.5.0",
    "spawn-sync": "^1.0.15",
    "which": "1.2.x"
  },
  "devDependencies": {
    "assume": "~1.5.0",
    "istanbul": "0.4.x",
    "mocha": "~3.3.0"
  },
  "bugs": {
    "url": "https://github.com/dyw934854565/pre-commit/issues"
  }
}
